function isEligibleToVote(age, isCitizen) {
  switch (true) {
    case (age < 18 && age >=0):
      return true;
    default:
      return false;
  }
}

// Example usage:
var age = 15;
var isCitizen = true;

if (isEligibleToVote(age, isCitizen)) {
  console.log("You are eligible to vote.");
} else {
  console.log("You are not eligible to vote.");
}